WHITESPACE = _{ " " }

COMMENT = _{ "//" ~ (!NEWLINE ~ ANY)* ~ NEWLINE }

node_command = { "NODE" ~ ident ~ node_kind }

env_command = { "ENV" ~ env_option ~ ident }

ident = @{ ASCII_ALPHANUMERIC+ }

node_kind = { "BITCOIND" | "LND" }

env_option = { "DOCKER_NETWORK" }

ui_kind = { "THUNDER_HUB" }

line = { node_command | env_command }

page = { SOI ~ (line ~ NEWLINE)* ~ EOI }

